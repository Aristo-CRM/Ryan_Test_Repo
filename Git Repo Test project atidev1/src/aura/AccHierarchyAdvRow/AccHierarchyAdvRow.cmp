<aura:component >

	<aura:attribute access="public" name="account" type="Object" />
	<aura:attribute access="public" name="level" type="integer" />
	<aura:attribute access="public" name="icon" type="string" />
	
	<tr aria-level="{!v.level}" aria-posinset="1" aria-setsize="4" class="slds-hint-parent" tabindex="0">
      <th class="slds-tree__item" data-label="Account Name" scope="row">
		<lightning:icon iconName="utility:chevronright" variant="default" size="x-small"/>
